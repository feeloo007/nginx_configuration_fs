{
    "$schema"		: "http://json-schema.org/draft-04/schema#",
    "title"            	: "backed_uri_extra",
    "type"             	: "object",
    "properties"    	:
        {
             "proxy_buffering"		:
                          {
                               "type"          : "string",
                               "enum"          : [ "on", "off" ],
                               "default"       : "on"
                          },
             "proxy_connect_timeout"	:
                          {
                               "type"          : "string",
                               "enum"          : [ "2s" ],
                               "default"       : "2s"
                          },
             "proxy_read_timeout"	:
                          {
                               "type"          : "string",
                               "enum"          : [ "10s", "65s", "125s", "1805s" ],
                               "default"       : "10s"
                          },
             "client_http_connect"	:
                          {
                               "type"          : "string",
                               "enum"          : [ "", "close" ],
                               "default"       : ""
                          },
             "mdp_service_redirector_new_http_host"				:
                          {
                               "type"          : "string",
                               "default"       : ""
                          },
             "mdp_service_redirector_new_http_host_http_redirected_code"    :
                          {
                               "type"          : "number",
                               "enum"          : [ 301, 302 ],
                               "default"       : 302
                          },
             "mdp_service_redirector_new_http_host_http_redirected_port"    :
                          {
                               "type"          : "number",
                               "default"       : "same"
                          },
             "mdp_service_redirector_new_http_host_http_redirected_proto"    :
                          {
                               "type"          : "string",
                               "enum"          : [ "http", "https", "same" ],
                               "default"       : "same"
                          }
        }
}
